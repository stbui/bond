{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "github": "gh-pages -d dist",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@antv/g2plot": "^2.4.16",
    "d3": "^7.4.0",
    "d3-dsv": "^3.0.1",
    "d3-format": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "decimal.js": "^10.3.1",
    "eventemitter3": "^4.0.7",
    "moment": "^2.29.3",
    "react": "17.x",
    "react-dom": "17.x",
    "react-grid-layout": "^1.3.0",
    "react-stockcharts": "^0.7.8",
    "styled-components": "^5.3.3",
    "umi": "^3.5.20",
    "underscore": "^1.13.3"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.15",
    "@types/underscore": "^1.11.4",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "gh-pages": "^3.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
